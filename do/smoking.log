  9.         gen rmspe_post=sqrt(postmean) if year>1989
 10.         gen rmspe_pre=rmspe[_n-1] if year==1989
 11.         
.         mkmat rmspe_pre rmspe_post ratio if year==1989, matrix (state`i')
 12.                                                                 }
